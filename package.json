{
  "name": "prodimo-vscode",
  "displayName": "ProDiMoParam",
  "description": "VS Code tools for ProDiMo",
  "version": "0.0.1",
  "publisher": "Christian Rab",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrab/prodimo-vscode"
  },
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "prodimoparam",
      "aliases": ["ProDiMo Parameter.in", "prodimoparam"],
      "extensions": [".in"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "prodimoparam",
      "scopeName": "source.prodimoparam",
      "path": "./syntaxes/prodimoparam.tmLanguage.json"
    }]
  }
}
